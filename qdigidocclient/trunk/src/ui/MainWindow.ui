  <property name="styleSheet">
   <string notr="true">* {
font: 12px &quot;Arial, Liberation Sans&quot;;
color: #355670;
}

#background
{ background-image: url(&quot;:/images/background.png&quot;); }

/* widgets */
QDialogButtonBox { dialogbuttonbox-buttons-have-icons: 0; }

QPushButton
{
color: white;
background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #406A9E, stop: 0.75 #00355F);
border: 0px;
border-radius: 2px;
padding: 4px 7px;
margin-left: 1px;
margin-right: 1px;
min-width: 75px;
}

QPushButton:hover
{}

QPushButton:disabled
{
background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #A3AFC0, stop: 0.75 #80909D);
}

/* header */
#cards, #languages
{
color: white;
border: 0px;
padding-right: 5px;
margin-left: 1px;
margin-right: 1px;
background-color: #00355F;
}

#cards QAbstractItemView,
#languages QAbstractItemView,
#headerLine, #headerLine2
{
color: white;
background-color: #00355F;
}

#cards::down-arrow, #languages::down-arrow
{ image: url(&quot;:/images/languages_button.png&quot;); }

#cards::drop-down, #cards QAbstractItemView,
#languages::drop-down, #languages QAbstractItemView
{ border: 0px; }

#settings, #help
{
background-color: #00355F;
color: #afc6d1;
font: bold;
min-width: 0px;
}

#info
{
color: #668696;
font: bold;
border-radius: 3px;
background-color: rgba( 255, 255, 255, 153 );
padding: 10px;
}

/* content */
#viewSignatures
{
color: #71889A; 
border-left: 1px solid #DFDFDF;
}

#homeButtonLabel,
#viewContentLabel, #viewSignaturesLabel,
#signContentLabel, #signSigner, #signSignerLabel
{
color: #00355F;
font: bold;
}

#signContentView, #viewContentView
{
color: #355670;
background-color: transparent;
}

#home, #intro, #sign, #view,
#viewSignatures, #viewSignaturesScroll
{ background-color: transparent; }</string>
