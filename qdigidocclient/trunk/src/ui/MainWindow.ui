  <property name="styleSheet">
   <string notr="true">* {
color: #355670;
font: 12px &quot;Arial, Liberation Sans&quot;;
}

#background
{ background-image: url(&quot;:/images/background.png&quot;); }

/* widgets */
QDialogButtonBox
{ dialogbuttonbox-buttons-have-icons: 0; }

QPushButton
{
background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #406A9E, stop: 0.75 #00355F);
border: 0px;
border-radius: 2px;
color: white;
margin-left: 1px;
margin-right: 1px;
min-width: 75px;
padding: 1px 7px;
}

QDialogButtonBox QPushButton,
#home QPushButton, #utilityOpen
{ padding: 4px 7px; }

QPushButton:hover, QPushButton:focus
{ background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #F1C157, stop:0.28 #FADF91, stop:1 #EBA927); }

QPushButton:disabled
{ background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #A3AFC0, stop: 0.75 #80909D); }

/* header */
#cards, #languages
{
background-color: #00355F;
border: 0px;
color: white;
padding-right: 5px;
}

#cards QAbstractItemView,
#languages QAbstractItemView
{
background-color: white;
border: 0px;
color: #355670;
}

#headerLine, #headerLine2
{ background-color: #00355F; }

#cards::down-arrow, #languages::down-arrow
{ image: url(&quot;:/images/languages_button.png&quot;); }

#cards::drop-down,
#languages::drop-down
{ border: 0px; }

#settings, #help
{
background-color: #00355F;
color: #afc6d1;
font: bold;
min-width: 0px;
}

#info
{
background-color: rgba( 255, 255, 255, 153 );
border-radius: 3px;
color: #668696;
font: bold;
padding-left: 10px;
padding-right: 10px;
}

#utilityLabel
{
color: red;
font: bold;
}

/* content */
#homeButtonLabel,
#viewContentLabel, #viewSignaturesLabel,
#signContentLabel, #signSigner, #signSignerLabel
{
color: #00355F;
font: bold;
}

#signContentView, #viewContentView
{
background-color: transparent;
border: 1px solid #DFDFDF;
color: #355670;
}

#home, #intro, #sign, #view,
#viewSignatures
{ background-color: transparent; }

#viewSignatures
{ color: #71889A; }

#signSignerRole, #viewSignaturesScroll
{
background-color: transparent;
border: 1px solid #DFDFDF;
}</string>
